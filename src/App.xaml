<Application x:Class="ClipboardMonitor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ClipboardMonitor"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="TypeSingleLineText" Color="LightCyan" />
        <SolidColorBrush x:Key="TypeMultiLineText" Color="LightYellow" />
        <SolidColorBrush x:Key="TypeImage" Color="Chartreuse" />
        <SolidColorBrush x:Key="TypeFiles" Color="Yellow" />
        <SolidColorBrush x:Key="TypeError" Color="Orange" />
        <SolidColorBrush x:Key="TypeUnknown" Color="Orange" />
        <SolidColorBrush x:Key="TypeEmpty" Color="White" />
    </Application.Resources>
</Application>
